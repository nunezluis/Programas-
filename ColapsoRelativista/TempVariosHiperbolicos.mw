<Group hide-input="false" labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">AA2  := sqrt((-r^2+2*m*r+a^2*cosh(theta)^2)^2/((cosh(theta)*a-r)^2*(cosh(theta)*a+r)^2)): simplify(%);</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L3" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">RR2 := -(a^4*(a^2+2*m*r-r^2)*cosh(theta)^4-2*a^2*r^2*(a^2+2*m^2+2*m*r-r^2)*cosh(theta)^2+4*a^2*m^2*r^2+a^2*r^4+2*m*r^5-r^6)*sinh(theta)^2/(sqrt((-r^2+2*m*r+a^2*cosh(theta)^2)^2/((cosh(theta)*a-r)^2*(cosh(theta)*a+r)^2))*(cosh(theta)*a-r)^2*(cosh(theta)*a+r)^2): simplify(%);</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L4" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">omega3 := 2*sqrt((-r^2+2*m*r+a^2*cosh(theta)^2)^2/((cosh(theta)*a-r)^2*(cosh(theta)*a+r)^2))*m*a*r*sinh(theta)^2/(-r^2+2*m*r+a^2*cosh(theta)^2): simplify(%);</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L5" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">term1 := simplify(AA2*RR2);</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L6" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">omega32 := simplify(omega3^2)</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L7" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Phi2:= simplify(term1 +omega32);factor(%);</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L8" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
